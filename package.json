{
  "name": "aoc-2023",
  "description": "Advent of Code 2023",
  "keywords": [
    "adventofcode",
    "aoc",
    "bun",
    "biome",
    "biomejs"
  ],
  "license": "ISC",
  "author": "George Achkasov",
  "scripts": {
    "lint": "biome lint ./",
    "format": "biome format --write ./",
    "format:check": "biome format ./",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "type-check": "tsc --noEmit",
    "ci": "biome ci . && bun run type-check && bun run test"
  },
  "engines": {
    "bun": "~1.0.14"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "bun-types": "^1.0.14"
  },
  "type": "module",
  "dependencies": {
    "typescript": "^5.3.2"
  }
}
